#include<stdio.h>
void main()
{
  char c;
  printf("Enter a character:");
  scanf("%c,&c);
  if(c=='a'||c=='A'||c=='e'||c=='E'||c=='i'||c=='I'||c=='o'||c=='O'||c=='u'||c=='U')
  {
    printf("The entered charater is a vowel.");
  }
  else
  {
    printf("The entered character is not a vowel.");
  }
}
